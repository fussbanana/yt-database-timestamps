/* Global Font Settings */
* {
  font-family: "Inter", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, sans-serif;
  font-size: 14px;
}

/* Main Window Styling */
QMainWindow {
  background-color: #2c313a;
  color: #f0f0f0;
  border: none;
}

/* Central Widget Container */
#centralwidget {
  background-color: #363b43;
  border-radius: 4px;
  border: 1px solid #4b4f57;
  margin: 4px;
}

/* Enhanced Button Styling */
QPushButton {
  background-color: #54a0ff;
  color: #f0f0f0;
  border: none;
  border-radius: 4px;
  padding: 8px 16px;
  font-weight: 500;
  font-size: 14px;
  min-height: 20px;
}

QPushButton:hover {
  background-color: #4380cc;
}

QPushButton:pressed {
  background-color: #326099;
}

QPushButton:disabled {
  background-color: #41454d;
  color: #909090;
}

/* Secondary Button Style */
QPushButton[class=secondary] {
  background-color: #41454d;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
}

QPushButton[class=secondary]:hover {
  background-color: #4b4f57;
  border: 1px solid #54a0ff;
}

/* Input Fields */
QLineEdit,
QTextEdit,
QPlainTextEdit {
  background-color: #41454d;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  padding: 6px 10px;
}

QLineEdit:focus,
QTextEdit:focus,
QPlainTextEdit:focus {
  border: 1px solid #54a0ff;
  background-color: #4b4f57;
}

/* ComboBox Styling */
QComboBox {
  background-color: #41454d;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  padding: 6px 10px;
  min-width: 120px;
}

QComboBox:hover {
  border: 1px solid #54a0ff;
}

QComboBox::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  border: none;
  border-left: 1px solid #4b4f57;
  border-radius: 0px;
  width: 20px;
  margin: 1px;
}

QComboBox::drop-down {
  subcontrol-origin: padding;
  subcontrol-position: top right;
  border: none;
  border-left: 1px solid #4b4f57;
  border-radius: 0px;
  width: 20px;
  margin: 1px;
}

QComboBox::down-arrow {
  image: url(":/feather/arrow_down.png");
  width: 10px;
  height: 10px;
}

QComboBox QAbstractItemView {
  background-color: #363b43;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
}

/* List and Tree Widgets */
QListWidget,
QTreeWidget {
  background-color: #363b43;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
}

QListWidget::item,
QTreeWidget::item {
  padding: 4px 8px;
  border-radius: 2px;
  margin: 1px;
}

QListWidget::item:selected,
QTreeWidget::item:selected {
  background-color: #54a0ff;
  color: #f0f0f0;
}

QListWidget::item:hover,
QTreeWidget::item:hover {
  background-color: #214066 !important;
  color: #f0f0f0 !important;
  outline: 0;
}

