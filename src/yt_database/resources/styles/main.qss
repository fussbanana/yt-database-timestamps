--------------------------------------------------------------------------- */
/* (dot) .QFrame  fix #141, #126, #123 */
.QFrame {
  border-radius: 4px;
  /* No frame */
  /* HLine */
  /* HLine */
}
.QFrame[frameShape="0"] {
  border-radius: 4px;
  border: 1px transparent #4b4f57;
}
.QFrame[frameShape="4"] {
  max-height: 2px;
  border: none;
  background-color: #4b4f57;
}
.QFrame[frameShape="5"] {
  max-width: 2px;
  border: none;
  background-color: #4b4f57;
}

/* QSplitter --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qsplitter

--------------------------------------------------------------------------- */
QSplitter {
  background-color: #4b4f57;
  spacing: 0px;
  padding: 0px;
  margin: 0px;
}
QSplitter::handle {
  background-color: #4b4f57;
  border: 0px solid #2c313a;
  spacing: 0px;
  padding: 1px;
  margin: 0px;
}
QSplitter::handle:hover {
  background-color: #606060;
}
QSplitter::handle:horizontal {
  width: 5px;
