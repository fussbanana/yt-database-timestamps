/* MAIN QSS STYLESHEET------------------------------------------ */
/*

See Qt documentation:

  - https://doc.qt.io/qt-5/stylesheet.html
  - https://doc.qt.io/qt-5/stylesheet-reference.html
  - https://doc.qt.io/qt-5/stylesheet-examples.html

--------------------------------------------------------------------------- */
/* Reset elements ------------------------------------------------------------

Resetting everything helps to unify styles across different operating systems

--------------------------------------------------------------------------- */
* {
  padding: 0px;
  margin: 0px;
  border: 0px;
  border-style: none;
  border-image: none;
  outline: 0;
  background-color: transparent;
  background: transparent;
}

/* specific reset for elements inside QToolBar */
QToolBar * {
  margin: 0px;
  padding: 0px;
}

/* QWidget ----------------------------------------------------------------

--------------------------------------------------------------------------- */
QWidget {
  background-color: #2c313a;
  border: 0px solid #4b4f57;
  padding: 0px;
  color: #f0f0f0;
  selection-background-color: #4380cc;
  selection-color: #f0f0f0;
}
QWidget:disabled {
  background-color: #2c313a;
  color: #606060;
  selection-background-color: #54a0ff;
  selection-color: #606060;
}
QWidget::item:selected {
  background-color: #4380cc;
}
QWidget::item:hover:not(selected) {
  background-color: #326099;
}

/* QMainWindow ------------------------------------------------------------

This adjusts the splitter in the dock widget, not qsplitter
https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qmainwindow

--------------------------------------------------------------------------- */
QMainWindow::separator {
  background-color: #4b4f57;
  border: 0px solid #2c313a;
  spacing: 0px;
  padding: 2px;
}
QMainWindow::separator:hover {
  background-color: #60646b;
  border: 0px solid #326099;
}
QMainWindow::separator:horizontal {
  width: 5px;
  margin-top: 2px;
  margin-bottom: 2px;
  image: url(":/feather/toolbar_separator_vertical.png");
}
QMainWindow::separator:vertical {
  height: 5px;
  margin-left: 2px;
  margin-right: 2px;
  image: url(":/feather/toolbar_separator_horizontal.png");
}

/* QToolTip ---------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qtooltip

--------------------------------------------------------------------------- */
QToolTip {
  background-color: #4380cc;
  color: #f0f0f0;
  /* If you remove the border property, background stops working on Windows */
  border: none;
  /* Remove padding, for fix combo box tooltip */
  padding: 2px;
  border: 1px solid #60646b;
  border-radius: 3px;
  /* Remove opacity, fix #174 - may need to use RGBA */
}

/* QStatusBar -------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qstatusbar

--------------------------------------------------------------------------- */
QStatusBar {
  border: 1px solid #4b4f57;
  /* Fixes Spyder #9120, #9121 */
  background: #4b4f57;
  /* Fixes #205, white vertical borders separating items */
}
QStatusBar::item {
  border: none;
}
QStatusBar QToolTip {
  background-color: #326099;
  border: 1px solid #2c313a;
  color: #2c313a;
  /* Remove padding, for fix combo box tooltip */
  padding: 0px;
  /* Reducing transparency to read better */
  opacity: 230;
}
QStatusBar QLabel {
  /* Fixes Spyder #9120, #9121 */
  background: transparent;
}

/* QCheckBox --------------------------------------------------------------

https://doc.qt.io/qt-5/stylesheet-examples.html#customizing-qcheckbox

--------------------------------------------------------------------------- */
QCheckBox {
  background-color: #2c313a;
  color: #f0f0f0;
  spacing: 4px;
  outline: none;
  padding-top: 4px;
  padding-bottom: 4px;
}
QCheckBox:focus {
  border: none;
}
QCheckBox QWidget:disabled {
  background-color: #2c313a;
  color: #606060;
}
QCheckBox::indicator {
  margin-left: 2px;
  height: 14px;
  width: 14px;
}
QCheckBox::indicator:unchecked {
  image: url(":/feather/checkbox_unchecked.png");
}
QCheckBox::indicator:unchecked:hover, QCheckBox::indicator:unchecked:focus, QCheckBox::indicator:unchecked:pressed {
  border: none;
  image: url(":/feather/checkbox_unchecked.png");
}
QCheckBox::indicator:unchecked:disabled {
  image: url(":/feather/checkbox_unchecked.png");
}
QCheckBox::indicator:checked {
  image: url(":/feather/checkbox_checked.png");
}
QCheckBox::indicator:checked:hover, QCheckBox::indicator:checked:focus, QCheckBox::indicator:checked:pressed {
  border: none;
  image: url(":/feather/checkbox_checked.png");
}
QCheckBox::indicator:checked:disabled {
  image: url(":/feather/checkbox_checked.png");
}
QCheckBox::indicator:indeterminate {
  image: url(":/feather/checkbox_indeterminate.png");
}
QCheckBox::indicator:indeterminate:disabled {
  image: url(":/feather/checkbox_indeterminate.png");
}
QCheckBox::indicator:indeterminate:focus, QCheckBox::indicator:indeterminate:hover, QCheckBox::indicator:indeterminate:pressed {
  image: url(":/feather/checkbox_indeterminate.png");
}

