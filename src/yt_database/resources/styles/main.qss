QTableWidget {
  background-color: #363b43;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
}

QTableWidget::item {
  padding: 6px 8px;
  border: none;
}

QTableWidget::item:selected {
  background-color: #54a0ff;
  color: #f0f0f0;
}

QTableWidget::item:hover {
  background-color: #214066 !important;
  color: #f0f0f0 !important;
  outline: 0;
}

QTableWidget::item:selected:hover {
  background-color: #4380cc;
  color: #f0f0f0;
}

QHeaderView::section {
  background-color: #4b4f57;
  color: #f0f0f0;
  border: none;
  border-right: 1px solid #4b4f57;
  border-bottom: 1px solid #4b4f57;
  padding: 8px 12px;
