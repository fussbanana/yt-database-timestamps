  border: 1px solid #4b4f57;
}

QSplitter::handle:horizontal {
  width: 4px;
  margin: 2px 0px;
}

QSplitter::handle:vertical {
  height: 4px;
  margin: 0px 2px;
}

/* Group Box */
QGroupBox {
  background-color: #363b43;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  margin-top: 10px;
  padding-top: 8px;
  font-weight: 600;
}

QGroupBox::title {
  padding: 0 8px;
  background-color: #363b43;
  color: #f0f0f0;
}

/* Tab Widget */
QTabWidget::pane {
  background-color: #363b43;
  border: 1px solid #4b4f57;
  border-radius: 4px;
}

QTabBar::tab {
  background-color: #41454d;
  color: #c0c0c0;
  border: 1px solid #4b4f57;
  border-bottom: none;
  padding: 8px 16px;
  margin-right: 2px;
}

QTabBar::tab:selected {
  background-color: #54a0ff;
  color: #f0f0f0;
}

QTabBar::tab:hover {
  background-color: #4b4f57;
  color: #f0f0f0;
}

/* Scroll Bars */
QScrollBar:vertical {
  background-color: #363b43;
  width: 12px;
  border-radius: 6px;
  margin: 0px;
}

QScrollBar::handle:vertical {
  background-color: #4b4f57;
  border-radius: 6px;
  min-height: 20px;
}

QScrollBar::handle:vertical:hover {
  background-color: #565a61;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
  height: 0px;
}

QScrollBar:horizontal {
  background-color: #363b43;
  height: 12px;
  border-radius: 6px;
  margin: 0px;
}

QScrollBar::handle:horizontal {
  background-color: #4b4f57;
  border-radius: 6px;
  min-width: 20px;
}

QScrollBar::handle:horizontal:hover {
  background-color: #565a61;
}

QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
  width: 0px;
}

/* Progress Bar */
QProgressBar {
  background-color: #41454d;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  text-align: center;
  padding: 2px;
}

QProgressBar::chunk {
  background-color: #54a0ff;
  border-radius: 4px;
}

/* Menu Bar and Menus */
QMenuBar {
  background-color: #2c313a;
  color: #f0f0f0;
  border-bottom: 1px solid #4b4f57;
}

QMenuBar::item {
  background-color: transparent;
  padding: 6px 12px;
}

QMenuBar::item:selected {
  background-color: #41454d;
}

QMenu {
  background-color: #41454d;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  padding: 4px;
}

QMenu::item {
  padding: 6px 20px;
  border-radius: 2px;
}

QMenu::item:selected {
  background-color: #54a0ff;
  color: #f0f0f0;
}

/* Status Bar */
QStatusBar {
  background-color: #2c313a;
  color: #c0c0c0;
  border-top: 1px solid #4b4f57;
  padding: 4px 8px;
}

/* Tool Tips */
QToolTip {
  background-color: #4b4f57;
  color: #f0f0f0;
  border: 1px solid #4b4f57;
  border-radius: 4px;
  padding: 6px 8px;
}

/* Check Box */
QCheckBox {
  color: #f0f0f0;
}

/* Radio Button */
QRadioButton {
  color: #f0f0f0;
}

QRadioButton::indicator {
  width: 16px;
  height: 16px;
  border: 1px solid #4b4f57;
  border-radius: 8px;
